{
  "name": "service-platform-backend",
  "version": "1.0.0",
  "description": "Service Platform Backend ",
  "main": "dist/src/server.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/server.ts",
    "dev:watch": "tsx watch src/server.ts",
    "build": "tsc",
    "build:render": "rm -rf dist && npm install --include=dev && npm run prisma:generate && npm run build",
    "start": "node dist/src/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push",
    "prisma:seed": "tsx prisma/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config jest.e2e.config.js",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "home-service",
    "category-based",
    "all-service-available"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^5.22.0",
    "@sendgrid/mail": "^8.1.6",
    "@sentry/node": "^10.32.0",
    "@sentry/profiling-node": "^10.32.0",
    "@types/socket.io": "^3.0.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.9.7",
    "resend": "^6.4.2",
    "socket.io": "^4.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.3",
    "baseline-browser-mapping": "^2.8.32",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "prisma": "^5.7.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}